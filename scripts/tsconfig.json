{
    "compilerOptions": {
        "target": "es2016",
        "jsx": "react",
        "lib": [
            "es2020",
            "dom",
            "dom.iterable"
        ],
        "outDir": "./bin",
        "types": [
            "cypress",
            "cypress-axe",
            "@percy/cypress",
            "@types/node"
        ],
        "resolveJsonModule": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "strictPropertyInitialization": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "module": "commonjs"
    },
    "include": [
        "**/*.ts"
    ]
}

{
  "compilerOptions": {
    "experimentalDecorators": false,
    "emitDecoratorMetadata": false,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es2016",
    "noImplicitAny": false,
    "noUnusedLocals": true,
    "sourceMap": false,
    "outDir": "./bin",
    "declaration": true,
    "jsx": "react",
    "lib": [
      "es2020",
      "dom",
      "dom.iterable"
    ],
  },
  "include": [
    "./**/*.ts",
  ],
}

    "has": "^1.0.3",
    "object-is": "^1.1.2"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-replace-object-assign": "^2.0.0",
    "babel-preset-airbnb": "^4.5.0",
    "eslint": "^7.6.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "in-publish": "^2.0.1",
    "rimraf": "^2.7.1",
    "safe-publish-latest": "^1.1.4"
  }

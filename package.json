{
  "name": "matrix-react-sdk",
  "version": "3.52.0",
  "description": "SDK for matrix.org using React",
  "author": "matrix.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/matrix-org/matrix-react-sdk"
  },
  "license": "Apache-2.0",
  "files": [
    "lib",
    "res",
    "src",
    "scripts",
    "git-revision.txt",
    "docs",
    "header",
    "CHANGELOG.md",
    "CONTRIBUTING.rst",
    "LICENSE",
    "README.md",
    "package.json",
    ".stylelintrc.js"
